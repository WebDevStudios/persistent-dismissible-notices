{
  "name": "webdevstudios/persistent-dismissible-notices",
  "description": "Persistent Dismissible Notices is a free WordPressÂ® plugin that enhances the handling of admin notices by making them dismissible and ensuring their dismissal persists across sessions.",
  "type": "wordpress-plugin",
  "config": {
    "platform": {
      "php": "8.0"
    },
    "allow-plugins": {
      "composer/installers": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "squizlabs/php_codesniffer": true,
      "wp-coding-standards/wpcs": true
    },
    "sort-packages": true
  },
  "authors": [
    {
      "name": "WebDevStudios",
      "email": "hosting@webdevstudios.com",
      "role": "Developer"
    },
    {
      "name": "Robert DeVore",
      "role": "Developer Emeritus"
    }
  ],
  "license": "GPL-2.0-or-later",
  "require": {
    "composer/installers": "^2.3.0",
    "php": ">=8.0"
  },
  "require-dev": {
    "php": ">=8.0",
    "dealerdirect/phpcodesniffer-composer-installer": "^1.0.0",
    "phpcompatibility/phpcompatibility-wp": "^2.1.6",
    "squizlabs/php_codesniffer": "^3.12.1",
    "wp-cli/wp-cli-bundle": "^2.11.0",
    "wp-coding-standards/wpcs": "^3.1.0"
  },
  "scripts": {
    "phpcs": [
      "@php vendor/bin/phpcs --report=full,source"
    ],
    "phpcs-fix": [
      "@php vendor/bin/phpcbf --report=summary,source"
    ]
  }
}
